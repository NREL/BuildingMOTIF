:fc-6 a sh:NodeShape, owl:Class, bmotif:Analytics_Application ;
    bmotif:domain bmotif:HVAC ;
    rdfs:label "FC#6" ;
    rdfs:comment "OA fraction is too low or too high; should equal %OA_MIN" ;
    sh:class brick:Air_Handling_Unit ;
    sh:property [
        rdfs:label "RAT_AVG" ;
        rdfs:comment "rolling average of return air temperature" ;
        sh:path brick:hasPoint ;
        sh:qualifiedValueShape [ sh:class brick:Return_Air_Temperature_Sensor ] ;
        sh:qualifiedMinCount 1 ;
    ] ;
    sh:property [
        rdfs:label "OAT_AVG" ;
        rdfs:comment "rolling average of outdoor air temperature" ;
        sh:path brick:hasPoint ;
        sh:qualifiedValueShape [ sh:class brick:Outside_Air_Temperature_Sensor ] ;
        sh:qualifiedMinCount 1 ;
    ] ;
    # sh:property [
    #     rdfs:label "Delta_T_MIN" ;
    #     rdfs:comment "Minimum difference between OAT and RAT to evaluate economizer error conditions (FC#6)" ;
    #     sh:path brick:hasPoint ;
    #     TODO missing from brick
    #     sh:qualifiedValueShape [ sh:class brick:Air_Differential_Temperature_Sensor ] ;
    #     sh:qualifiedMinCount 1 ;
    # ] ;
    sh:property [
        rdfs:label "%OA" ;
        rdfs:comment "actual outdoor air fraction as a percentage = (MAT - RAT)/(OAT - RAT), or per airflow measurement station if available." ;
        sh:path brick:hasPoint ;
        sh:qualifiedValueShape [
            sh:or (
                sh:and (
                    sh:class brick:Mixed_Air_Temperature_Sensor
                    sh:class brick:Return_Air_Temperature_Sensor
                    sh:class brick:Outside_Air_Temperature_Sensor
                )
                sh:class brick:Outside_Air_Flow_Sensor
            ) ;
        ] ;
        sh:qualifiedMinCount 1 ;
    ] .

